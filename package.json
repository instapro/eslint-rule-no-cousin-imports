{
  "name": "eslint-rule-no-cousin-imports",
  "version": "1.0.0",
  "description": "ESLint rule to prevent cousin imports between sibling directory trees",
  "main": "src/no-cousin-imports.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix"
  },
  "keywords": [
    "eslint",
    "eslint-rule",
    "import",
    "architecture",
    "module-boundaries",
    "cousin-imports",
    "javascript",
    "typescript"
  ],
  "author": "Instapro Group",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/instapro/eslint-rule-no-cousin-imports.git"
  },
  "bugs": {
    "url": "https://github.com/instapro/eslint-rule-no-cousin-imports/issues"
  },
  "homepage": "https://github.com/instapro/eslint-rule-no-cousin-imports#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "eslint": "^8.0.0 || ^9.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.0",
    "eslint": "^9.29.0",
    "jest": "^30.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  }
}
